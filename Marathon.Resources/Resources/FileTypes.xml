<?xml version="1.0" encoding="utf-8" ?>
<Marathon>
  <!-- If no extension is specified or is a duplicate, the entry will be disregarded. -->
  <!-- File types that use common extensions should be appended to their respective types by name. -->
  <Type Extension=".*" Category="Any">All files</Type>
  <Type Extension=".arc" Category="Archives">U8 Archive</Type>
  <Type Extension=".bin" Category="Miscellaneous">Collision|Common Package|Explosion Package|Path Package|Save Data</Type>
  <Type Extension=".cpk" Category="Archives">Cri Pack File</Type>
  <Type Extension=".csb" Category="Sounds">Cue Sheet Binary</Type>
  <Type Extension=".ddm" Category="Textures">DirectDraw Matrix</Type>
  <Type Extension=".dds" Category="Textures">DirectDraw Surface</Type>
  <Type Extension=".epb" Category="Miscellaneous">Event Playbook</Type>
  <Type Extension=".ftm" Category="Text">Font Map</Type>
  <Type Extension=".lub" Category="Scripts">Lua Bytecode</Type>
  <Type Extension=".mst" Category="Text">Message Table</Type>
  <Type Extension=".path" Category="Miscellaneous">Path Spline</Type>
  <Type Extension=".pft" Category="Text">Picture Font</Type>
  <Type Extension=".pfi" Category="Text">Font Proportion</Type>
  <Type Extension=".pkg" Category="Miscellaneous">Asset Package</Type>
  <Type Extension=".plc" Category="Particles">Particle List Container</Type>
  <Type Extension=".prop" Category="Miscellaneous">Object Property Database</Type>
  <Type Extension=".ptb" Category="Particles">Particle Texture Bank</Type>
  <Type Extension=".rab" Category="Meshes">Reflection Zone</Type>
  <Type Extension=".sbk" Category="Sounds">Scene Bank</Type>
  <Type Extension=".set" Category="Placement">Object Placement</Type>
</Marathon>